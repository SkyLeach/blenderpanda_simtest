{
    "compilerOptions": {
        "module": "simgame",
        "noImplicitAny": true,
        "removeComments": false,
        "preserveConstEnums": true,
        "outFile": "../../built/local/tsc.js",
        "sourceMap": true
    },
    "include": [
        "**/*"
    ],
    "exclude": [
        "node_modules",
        "**/*.spec.ts",
        "**/*.pyc",
        "Library",
        "build",
        "dist",
        "__pycache__",
        "assets",
        "blender_scenes"
    ]
}
